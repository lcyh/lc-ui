{
  "name": "vue3-lcui",
  "version": "1.1.13",
  "license": "MIT",
  "description": "vue3-lcui components based on Vite and Vue3",
  "keywords": [
    "frontend",
    "typescript",
    "ui-component",
    "components",
    "vue-components",
    "vue",
    "vue3",
    "vite",
    "jsx",
    "vue3-lcui"
  ],
  "homepage": "https://lcyh.github.io/lc-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lcyh/lc-ui.git"
  },
  "main": "vue-lcui.umd.js",
  "module": "vue-lcui.mjs",
  "style": "style.css",
  "scripts": {
    "dev": "vitepress dev docs",
    "predev": "node ../cli/index.js create -t lcui --ignore-parse-error && node ../cli/index.js generate:theme",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vitepress-theme-demoblock": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.31.1",
    "@vue/test-utils": "^2.3.2",
    "@vueuse/core": "^10.1.2",
    "ant-design-vue": "^3.2.0",
    "c8": "^7.13.0",
    "jsdom": "^22.0.0",
    "moment": "^2.29.4",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.1.0",
    "vite-svg-loader": "^3.6.0",
    "vitepress": "1.0.0-alpha.75",
    "vitest": "^0.31.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.6.5"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.1.2",
    "ant-design-vue": "^3.2.0",
    "vue": "^3.2.33"
  }
}
