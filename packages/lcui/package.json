{
  "name": "vue3-lcui",
  "version": "1.1.5",
  "license": "MIT",
  "description": "vue3-lcui components based on Vite and Vue3",
  "keywords": [
    "frontend",
    "typescript",
    "ui-component",
    "components",
    "vue-components",
    "vue",
    "vue3",
    "vite",
    "jsx",
    "vue3-lcui"
  ],
  "homepage": "https://lcyh.github.io/lc-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lcyh/lc-ui.git"
  },
  "main": "vue-lcui.umd.js",
  "module": "vue-lcui.es.js",
  "style": "style.css",
  "scripts": {
    "dev": "vitepress dev docs",
    "predev": "node ../cli/index.js create -t lcui --ignore-parse-error && node ../cli/index.js generate:theme",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vitepress-theme-demoblock": "^1.4.2",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vueuse/core": "^9.13.0",
    "c8": "^7.11.3",
    "jsdom": "^20.0.0",
    "moment": "^2.29.4",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.3",
    "vite-svg-loader": "^3.4.0",
    "vitepress": "1.0.0-alpha.10",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.4"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.13.0"
  }
}